@model Proiect.Models.RestaurantViewModels.ChefIndexData

@{
    ViewData["Title"] = "New Chef";
}

<p>
    <a asp-action="Create">Create New Chef</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Chefs)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Chefs) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ChefName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
